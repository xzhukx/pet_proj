import random

#range from 0 to a

a = int(input("Please enter the last number in range from 0: "))
b = int(input("Please enter the last number in range from 0: "))

def rand_list(a,b):
    chet=[]
    nechet=[]
    for i in range(a,b):
        if a % 2 == 0:
            chet.append(a)
        else:
            nechet.append(a)
        i += 1
        a=a+1
    print(chet,nechet)

rand_list(a,b)